{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --retain-lines",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "npm run watch-compile",
    "build": "webpack -p"
  },
  "dependencies": {
    "thinkjs": "2.1.x",
    "babel-runtime": "6.x.x"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-1": "6.x.x",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "moment": "^2.11.1",
    "replace-webpack-plugin": "^0.1.2",
    "request-promise": "^2.0.0",
    "sass-loader": "^3.1.2",
    "vue": "^1.0.15",
    "vue-resource": "^0.6.1",
    "vue-router": "^0.7.10",
    "vue-validator": "^1.4.4"
  }
}
