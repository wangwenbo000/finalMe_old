<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title><![CDATA[{{title}}]]></title>
    <link href="{{domain}}/rss.html" rel="self"/>
    <link href="{{domain}}"/>
    <updated>{{updateTime}}</updated>
    <id>{{domain}}</id>
    <author>
        <name><![CDATA[{{siteInfo.website_author_name }}]]></name>
    </author>
    <generator uri="http://wangwenbo.me/">wangwenbo.me</generator>
    {% for post in list %}
    <entry>
        <title type="html"><![CDATA[{{title}}]]></title>
        <link href="{{domain}}/{{ post.routename }}"/>
        <updated>{{post.lastdate }}</updated>
        <id>{{domain}}/{{ post.routename }}</id>
        <content type="html">
            {{post.content}}
            <a href="{{domain}}/{{ post.routename }}">阅读全文</a>
        </content>
    </entry>
    {% endfor %}
</feed>